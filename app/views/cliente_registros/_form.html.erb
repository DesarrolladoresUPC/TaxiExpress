<%= simple_form_for(@cliente_registro) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :telefono %>
    <%= f.input :apellido %>
    <%= f.input :correo %>
    <%= f.input :contrasena %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
