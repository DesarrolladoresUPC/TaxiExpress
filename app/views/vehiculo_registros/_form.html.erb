<%= simple_form_for(@vehiculo_registro) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :placa %>
    <%= f.input :marca %>
    <%= f.input :modelo %>
    <%= f.input :color %>
    <%= f.input :categoria %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
